{
  "parser": "@typescript-eslint/parser",
  "extends": ["react-app", "prettier"],
  "plugins": ["@typescript-eslint", "prettier"],
  "ignorePatterns": ["node_modules/"],
  "env": {
    "browser": true,
    "node": true
  },
  "rules": { "@typescript-eslint/explicit-module-boundary-types": "off" }
}
